<h2><%= raw l :newissuealerts_edit, :project => h(@project.name) %></h2>
<%= labelled_form_for @newissuealert, url: newissuealert_path(@project, @newissuealert) do |f| %>
  <%= error_messages_for 'newissuealert' %>
  <div class="box tabular settings">
    <%= render :partial => 'form', :locals => { :f => f } %>
    <p>
      <%= submit_tag l(:save), :name => "save" %>
    </p>
  </div>
<% end %>

